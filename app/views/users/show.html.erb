<div class="container">
  <div class="row">
    <div class="col-md-10 col-md-offset-1">
      <%= @user.profile.inspect %>
      
      <% if current_user.plan_id == 2 %>
        <div class="well">
          <h3 class="text-center">Pro Details</h3>
          <p class="text-center"><%= @user.profile.phone_number %></p>
          <p class="text-center"><%= @user.profile.contact_email %></p>
        </div>
      <% end %>
    </div>
  </div>
</div>